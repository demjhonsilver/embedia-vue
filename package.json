{
  "name": "embedia-vue",
  "version": "2.0.0",
  "description": "A vue component package to embed videos such as TikTok, YouTube/Shorts, Twitter/X, Vimeo, and Dailymotion.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/demjhonsilver/embedia-vue.git"
  },
  "homepage": "https://demjhonsilver.github.io/embedia-vue-docs/",
  "scripts": {
    "build": "vite build",
