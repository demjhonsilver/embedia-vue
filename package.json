    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/test-utils": "^2.4.0-alpha.2",
    "jsdom": "^27.1.0",
    "typescript": "^5.5.4",
    "vite": "^4.5.3",
    "vitest": "^4.0.8",
    "vue": "^3.4.38",
    "vue-tsc": "^2.1.4"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "keywords": [
    "embed",
